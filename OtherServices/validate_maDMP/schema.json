{
  "definitions": {},
  "$schema": "http://json-schema.org/draft-07/schema#",
  "$id": "http://example.com/root.json",
  "type": "object",
  "title": "The Root Schema",
  "required": [
    "dmp"
  ],
  "properties": {
    "dmp": {
      "$id": "#/properties/dmp",
      "type": "object",
      "title": "The Dmp Schema",
      "required": [
        "title",
        "description",
        "created",
        "modified",
        "project",
        "contact",
        "language",
        "ethical_issues_exist",
        "dataset"
      ],
      "properties": {
        "title": {
          "$id": "#/properties/dmp/properties/title",
          "type": "string",
          "title": "The Title Schema",
          "default": "",
          "examples": [
            "Funded DMP"
          ],
          "pattern": "^(.*)$"
        },
        "description": {
          "$id": "#/properties/dmp/properties/description",
          "type": "string",
          "title": "The Description Schema",
          "default": "",
          "examples": [
            "Example of a DMP header for a funded project."
          ],
          "pattern": "^(.*)$"
        },
        "created": {
          "$id": "#/properties/dmp/properties/created",
          "type": "string",
          "title": "The Created Schema",
          "default": "",
          "examples": [
            "2019-02-22T13:20:15.5"
          ],
          "pattern": "^(.*)$"
        },
        "modified": {
          "$id": "#/properties/dmp/properties/modified",
          "type": "string",
          "title": "The Modified Schema",
          "default": "",
          "examples": [
            "2019-02-22T15:10:56.9"
          ],
          "pattern": "^(.*)$"
        },
        "project": {
          "$id": "#/properties/dmp/properties/project",
          "type": "array",
          "title": "The Project Schema",
          "items": {
            "$id": "#/properties/dmp/properties/project/items",
            "type": "object",
            "title": "The Items Schema",
            "required": [
              "title",
              "project_start",
              "project_end",
              "funding"
            ],
            "properties": {
              "title": {
                "$id": "#/properties/dmp/properties/project/items/properties/title",
                "type": "string",
                "title": "The Title Schema",
                "default": "",
                "examples": [
                  "Making maDMPs awesome"
                ],
                "pattern": "^(.*)$"
              },
              "project_start": {
                "$id": "#/properties/dmp/properties/project/items/properties/project_start",
                "type": "string",
                "title": "The Project_start Schema",
                "default": "",
                "examples": [
                  "2017-01-01T00:00:00.0"
                ],
                "pattern": "^(.*)$"
              },
              "project_end": {
                "$id": "#/properties/dmp/properties/project/items/properties/project_end",
                "type": "string",
                "title": "The Project_end Schema",
                "default": "",
                "examples": [
                  "2020-12-31T00:00:00.0"
                ],
                "pattern": "^(.*)$"
              },
              "funding": {
                "$id": "#/properties/dmp/properties/project/items/properties/funding",
                "type": "array",
                "title": "The Funding Schema",
                "items": {
                  "$id": "#/properties/dmp/properties/project/items/properties/funding/items",
                  "type": "object",
                  "title": "The Items Schema",
                  "required": [
                    "funder_id",
                    "grant_id",
                    "funding_status"
                  ],
                  "properties": {
                    "funder_id": {
                      "$id": "#/properties/dmp/properties/project/items/properties/funding/items/properties/funder_id",
                      "type": "object",
                      "title": "The Funder_id Schema",
                      "required": [
                        "funder_id",
                        "funder_id_type"
                      ],
                      "properties": {
                        "funder_id": {
                          "$id": "#/properties/dmp/properties/project/items/properties/funding/items/properties/funder_id/properties/funder_id",
                          "type": "string",
                          "title": "The Funder_id Schema",
                          "default": "",
                          "examples": [
                            "501100002428"
                          ],
                          "pattern": "^(.*)$"
                        },
                        "funder_id_type": {
                          "$id": "#/properties/dmp/properties/project/items/properties/funding/items/properties/funder_id/properties/funder_id_type",
                          "type": "string",
                          "title": "The Funder_id_type Schema",
                          "default": "",
                          "examples": [
                            "FUNDREF"
                          ],
                          "pattern": "^(.*)$"
                        }
                      }
                    },
                    "grant_id": {
                      "$id": "#/properties/dmp/properties/project/items/properties/funding/items/properties/grant_id",
                      "type": "object",
                      "title": "The Grant_id Schema",
                      "required": [
                        "grant_id",
                        "grant_id_type"
                      ],
                      "properties": {
                        "grant_id": {
                          "$id": "#/properties/dmp/properties/project/items/properties/funding/items/properties/grant_id/properties/grant_id",
                          "type": "string",
                          "title": "The Grant_id Schema",
                          "default": "",
                          "examples": [
                            "1234567-AT"
                          ],
                          "pattern": "^(.*)$"
                        },
                        "grant_id_type": {
                          "$id": "#/properties/dmp/properties/project/items/properties/funding/items/properties/grant_id/properties/grant_id_type",
                          "type": "string",
                          "title": "The Grant_id_type Schema",
                          "default": "",
                          "examples": [
                            "custom"
                          ],
                          "pattern": "^(.*)$"
                        }
                      }
                    },
                    "funding_status": {
                      "$id": "#/properties/dmp/properties/project/items/properties/funding/items/properties/funding_status",
                      "type": "string",
                      "title": "The Funding_status Schema",
                      "default": "",
                      "examples": [
                        "granted"
                      ],
                      "pattern": "^(.*)$"
                    }
                  }
                }
              }
            }
          }
        },
        "contact": {
          "$id": "#/properties/dmp/properties/contact",
          "type": "object",
          "title": "The Contact Schema",
          "required": [
            "mail",
            "name",
            "contact_id"
          ],
          "properties": {
            "mail": {
              "$id": "#/properties/dmp/properties/contact/properties/mail",
              "type": "string",
              "title": "The Mail Schema",
              "default": "",
              "examples": [
                "TMiksa@sba-research.org"
              ],
              "pattern": "^(.*)$"
            },
            "name": {
              "$id": "#/properties/dmp/properties/contact/properties/name",
              "type": "string",
              "title": "The Name Schema",
              "default": "",
              "examples": [
                "Tomasz Miksa"
              ],
              "pattern": "^(.*)$"
            },
            "contact_id": {
              "$id": "#/properties/dmp/properties/contact/properties/contact_id",
              "type": "object",
              "title": "The Contact_id Schema",
              "required": [
                "contact_id",
                "contact_id_type"
              ],
              "properties": {
                "contact_id": {
                  "$id": "#/properties/dmp/properties/contact/properties/contact_id/properties/contact_id",
                  "type": "string",
                  "title": "The Contact_id Schema",
                  "default": "",
                  "examples": [
                    "https://orcid.org/0000-0000-0000-0000"
                  ],
                  "pattern": "^(.*)$"
                },
                "contact_id_type": {
                  "$id": "#/properties/dmp/properties/contact/properties/contact_id/properties/contact_id_type",
                  "type": "string",
                  "title": "The Contact_id_type Schema",
                  "default": "",
                  "examples": [
                    "HTTP-ORCID"
                  ],
                  "pattern": "^(.*)$"
                }
              }
            }
          }
        },
        "language": {
          "$id": "#/properties/dmp/properties/language",
          "type": "string",
          "title": "The Language Schema",
          "default": "",
          "examples": [
            "en"
          ],
          "pattern": "^(.*)$"
        },
        "ethical_issues_exist": {
          "$id": "#/properties/dmp/properties/ethical_issues_exist",
          "type": "string",
          "title": "The Ethical_issues_exist Schema",
          "default": "",
          "examples": [
            "no"
          ],
          "pattern": "^(.*)$"
        },
        "dataset": {
          "$id": "#/properties/dmp/properties/dataset",
          "type": "array",
          "title": "The Dataset Schema"
        }
      }
    }
  }
}